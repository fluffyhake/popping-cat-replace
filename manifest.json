{
    "manifest_version": 2,
    "name": "Replace with Popping Cat!",
    "permissions": ["storage", "activeTab"],
    "version": "6.9",
    "content_scripts": [
        {
          "matches": 
          ["<all_urls>"],
          "js": ["uber-replacer.js"]

        }
    ],
    "web_accessible_resources": ["popping.gif"],
    "browser_action":{
      "default_popup":"popup.html",
      "default_title": "POP POP POP"
    }
}