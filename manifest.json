{
    "manifest_version": 2,
    "name": "Replace with Popping Cat!",
    "permissions": ["storage"],
    "version": "6.9",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["uber-replacer.js"]
        }
    ],
    "page_action": {
      "default_icon": "popping.gif",
      "default_popup": "popup.html"
    },
    "web_accessible_resources": ["popping.gif"]
}